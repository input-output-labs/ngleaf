{
  "name": "ngleaf-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "clean": "rm -rf dist node_modules",
    "start": "ng serve",
    "start:prod": "ng serve --configuration production",
    "start:local": "ng serve --configuration local",
    "build": "ng lint && ng build",
    "build:lib": "ng build ngleaf --configuration production",
    "build:prod": "ng build --configuration=prod",
    "build:aot": "ng build --configuration production --aot --configuration=production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prettier": "prettier --single-quote --trailing-comma es5 --write \"**/*.{js,json,css,scss,md}\"",
    "publish": "npm publish --access public",
    "login": "npm login --registry=https://nexus.io-labs.fr/repository/npm-public/ --scope=@input-output-labs",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d ."
  },
  "pre-commit": {
    "run": []
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~18.2.13",
    "@angular/cdk": "~18.2.14",
    "@angular/common": "~18.2.13",
    "@angular/compiler": "~18.2.13",
    "@angular/core": "~18.2.13",
    "@angular/forms": "~18.2.13",
    "@angular/material": "~18.2.14",
    "@angular/platform-browser": "~18.2.13",
    "@angular/platform-browser-dynamic": "~18.2.13",
    "@angular/router": "~18.2.13",
    "@angular/service-worker": "~18.2.13",
    "@ngrx/effects": "~18.1.1",
    "@ngrx/store": "~18.1.1",
    "@ngrx/store-devtools": "~18.1.1",
    "@ngx-translate/core": "~15.0.0",
    "@ngx-translate/http-loader": "~8.0.0",
    "@stomp/rx-stomp": "~2.0.0",
    "@swimlane/ngx-charts": "~20.1.0",
    "awesome-phonenumber": "^7.2.0",
    "cookieconsent": "~3.1.1",
    "core-js": "~3.20.2",
    "lodash-es": "~4.17.21",
    "ngx-cookieconsent": "~2.2.3",
    "pre-commit": "~1.2.2",
    "prettier": "~2.5.1",
    "rxjs": "~7.5.2",
    "tslib": "^2.0.0",
    "zone.js": "~0.14.0",
    "@ngrx/operators": "^18.0.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~18.2.12",
    "@angular/cli": "~18.2.12",
    "@angular/compiler-cli": "~18.2.13",
    "@angular/language-service": "~18.2.13",
    "@babel/core": "~7.18.2",
    "@compodoc/compodoc": "~1.1.19",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~12.11.1",
    "babel-loader": "~8.2.5",
    "codelyzer": "~6.0.0",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.11",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "~18.2.1",
    "protractor": "~7.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "5.5.4"
  }
}