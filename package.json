{
  "name": "ngleaf-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "clean": "rm -rf dist node_modules",
    "start": "ng serve",
    "start:prod": "ng serve --prod",
    "build": "ng lint && ng build",
    "build:lib": "ng lint && ng build ngleaf --prod",
    "build:prod": "ng build --configuration=prod",
    "build:aot": "ng build --prod --aot --configuration=production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prettier": "prettier --single-quote --trailing-comma es5 --write \"**/*.{js,json,css,scss,md}\"",
    "publish": "npm publish --access public",
    "login": "npm login --registry=https://nexus.io-labs.fr/repository/npm-public/ --scope=@input-output-labs"
  },
  "pre-commit": {
    "run": [
      "lint"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.14",
    "@angular/cdk": "^10.2.7",
    "@angular/common": "~11.2.14",
    "@angular/compiler": "~11.2.14",
    "@angular/core": "~10.2.5",
    "@angular/forms": "~11.2.14",
    "@angular/material": "^10.2.7",
    "@angular/platform-browser": "~11.2.14",
    "@angular/platform-browser-dynamic": "~11.2.14",
    "@angular/router": "~11.2.14",
    "@ngrx/store": "^10.0.1",
    "@ngrx/store-devtools": "^10.0.1",
    "core-js": "^2.5.4",
    "pre-commit": "^1.2.2",
    "prettier": "^1.16.4",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.3",
    "@angular-devkit/build-ng-packagr": "~0.1002.3",
    "@angular/cli": "~10.2.3",
    "@angular/compiler-cli": "~11.2.14",
    "@angular/language-service": "~11.2.14",
    "@types/node": "^12.11.1",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^10.1.0",
    "protractor": "~7.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.8"
  }
}
